<?xml version="1.0" encoding="UTF-8"?>
<QOS>
	<CFG TYPE="REQ" REBUILD_TM="60"/>

	<!--成功率权重 时延权重-->
	<FACTOR RATE_WEIGHT="7" DELAY_WEIGHT="1"/>
	
	<!--请求超时时间 500ms-->
	<TIMEOUT REQ_TIMEOUT="500"/>

	<!--访问量的配置信息-->
	<REQ REQ_MAX="100000" REQ_MIN="10" REQ_ERR_MIN="0.1" REQ_EXTEND_RATE="0.2" PRE_TIME="4"/>

	<!--宕机控制&恢复配置-->
	<DOWN BEGIN="1" INTERVAL="10" EXPIRED="600" TIMES="3" REQ_COUNT="100000" DOWN_TIME="600" DOWN_ERR_REQ="10" DOWN_ERR_RATE="0.5"/>
</QOS>
